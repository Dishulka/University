# Sort Fragment Color Resource file
# Color available format:
# rgb:R/G/B (new) or #RGB (old) or gold (name)
# R, G, B := 1-4 hex digits

# Green All fragments
xsort.foreground: rgb:0000/FFFF/0000

# Blue (#0000FF) Rubber contour 
xsort.rubbground: #0000FF

# Red Extra fragment
xsort.extrground: rgb:FF/00/00

# Alt group fragment color

xsort.galtground: Blue

# Black (#0000) background for rubber out fragment
xsort.background: black
